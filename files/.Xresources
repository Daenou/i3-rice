URxvt.perl-ext-common: default,matcher
URxvt.url-launcher: /usr/bin/firefox
URxvt.matcher.button: 1

URxvt.scrollBar: false
 
URxvt*font: xft:Terminus:size=12
URxvt*boldFont: xft:Terminus:size=12

URxvt.letterSpace: +1
URxvt*scrollWithBuffer: false
URxvt.depth: 32
URxvt.pointerBlankDelay: 2

! special
*.foreground:   #c5c8c6
*.background:   [90]#1d1f21
*.cursorColor:  #db9501
 
! black
*.color0:       #1d1f21
*.color8:       #969896
 
! red
*.color1:       #cc342b
*.color9:       #cc342b
 
! green
*.color2:       #198844
*.color10:      #198844
 
! yellow
*.color3:       #fba922
*.color11:      #fba922
 
! blue
*.color4:       #3971ed
*.color12:      #3971ed
 
! magenta
*.color5:       #a36ac7
*.color13:      #a36ac7
 
! cyan
*.color6:       #0f73fb
*.color14:      #0f73fb
 
! white
*.color7:       #c5c8c6
*.color15:      #ffffff
